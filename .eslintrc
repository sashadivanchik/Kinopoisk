{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "quotes": [
            2,
            "single",
            { "allowTemplateLiterals": true }
        ],
        "jsx-quotes": [2, "prefer-single"]
    }
}
